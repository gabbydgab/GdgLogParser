{
    "name": "gabbydgab/gdg-logprovider",
    "description": "Abstract application that provides log information in an ETL process such as data logs",
    "license": "BSD-2-Clause",
    "version": "0.1.0-dev",
    "type": "library",
    "minimum-stability": "dev",
    "keywords": [
        "zf2 module",
        "log parsing"
    ],
    "homepage": "https://github.com/gabbydgab/GdgLogProvider",
    "authors": [
        {
            "name": "Gab Amba",
            "email": "gamba@gabbydgab.com",
            "homepage": "http://www.gabbydgab.com/"
        }
    ],
    "require": {
        "php": ">=5.4.20",
        "zendframework/zend-log": "2.2.*",
        "zendframework/zend-validator": "2.2.*",
        "zendframework/zend-config": "2.2.*",
        "zendframework/zend-filter": "2.2.*",
        "zendframework/zend-escaper": "2.2.*",
        "zf-commons/zfc-base": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "3.7.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "GdgLogProvider": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }    
}
